<format name="NeoGeo Pocket 2bpp">
    <codec>
        <colortype>indexed</colortype>
        <colordepth>2</colordepth>
        <imagetype>tiled</imagetype>
        <height>8</height>
        <width>8</width>
        <fixedsize>true</fixedsize>
		<mergepriority>0, 1</mergepriority>
    </codec>
    <image>
        <colordepth>2</colordepth>
		<rowpixelorder>4, 5, 6, 7, 0, 1, 2, 3</rowpixelorder>
        <rowinterlace>false</rowinterlace>
    </image>
</format>